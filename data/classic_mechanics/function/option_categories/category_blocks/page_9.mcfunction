execute if score CM.global CM_OldTNT matches 0 run tellraw @s ["",{"text":"\u25b6 ","color":"gold"},{"color":"#FF3333","text":"● "},{"text":"{Old TNT ignition}","bold":true,"color":"#FF9E0C","hoverEvent":{"action":"show_text","value":[{"text":"DESCRIPTION:\n","color":"#F8A12F","bold":true},{"text":"Prior to B1.7, TNT blocks were ignited by punching it, this was changed to make it so only redstone or punching it with a flint and steel would be able to ignite it, and later on, this was changed to redstone signals and right clicking with flint and steels/fire charges!","color":"#F8D563","bold":false}]}},{"text":"\n"},{"text":"\u2514 ","color":"gold"},{"text":"C0.26-B1.7","color":"#F8DB00"},{"text":"\n"},{"text":"\u2514 ","color":"gold"},{"text":"[Enable]","color":"#2AA946","clickEvent":{"action":"run_command","value":"/function classic_toggles:toggles/enable_with_data/old_tnt"},"hoverEvent":{"action":"show_text","value":[{"text":">Click to enable","color":"#1AC926","bold":true}]}},{"text":" ","color":"gold"},{"text":"[Disable]","color":"#F9403E","clickEvent":{"action":"run_command","value":"/function classic_toggles:toggles/disable_generic/disable_module_1 {\"score\":\"CM_OldTNT\",\"function1\":\"classic_mechanics:blocks_modules/old_tnt/old_tnt\",\"module\":\"old tnt\",\"page\":\"category_blocks/page_9\"}"},"hoverEvent":{"action":"show_text","value":[{"text":">Click to disable","color":"#E02626","bold":true}]}},{"text":"\n "}]
